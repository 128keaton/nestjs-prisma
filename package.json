{
  "name": "nestjs-prisma",
  "version": "0.7.0",
  "description": "Schematics to add Prisma support to a NestJS application",
  "scripts": {
    "build": "npm run build:lib && npm run build:schematics",
    "build:lib": "tsc -p tsconfig.lib.json",
    "build:schematics": "tsc -p tsconfig.schematics.json",
    "dev:schematics": "tsc -p tsconfig.schematics.json --watch",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "changelog": "conventional-changelog -p angular -i ./CHANGELOG.md -s",
    "rocket": "npm run build && npm publish"
  },
  "keywords": [
    "nestjs",
    "prisma",
    "schematics"
  ],
  "author": "Marc Stammerjohann",
  "license": "MIT",
  "main": "./dist/index.js",
  "schematics": "./schematics/collection.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcjulian/nestjs-prisma.git"
  },
  "bugs": {
    "url": "https://github.com/marcjulian/nestjs-prisma/issues"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.0",
    "@prisma/client": "^2.13.0",
    "@prisma/cli": "^2.13.0"
  },
  "devDependencies": {
    "@nestjs/common": "7.6.1",
    "@nestjs/core": "7.6.1",
    "@prisma/cli": "2.13.0",
    "@prisma/client": "2.13.0",
    "@types/jasmine": "3.6.2",
    "@types/node": "^12.19.7",
    "jasmine": "^3.6.3",
    "prettier": "2.2.1",
    "reflect-metadata": "0.1.13"
  },
  "dependencies": {
    "@angular-devkit/core": "^11.0.4",
    "@angular-devkit/schematics": "^11.0.4",
    "@schematics/angular": "~11.0.4",
    "typescript": "~4.0.5"
  }
}
